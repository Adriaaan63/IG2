material cuerpoAvion{
    technique{
        pass{
            ambient 0.5 0.5 0.5 
            diffuse 1.0 1.0 1.0
            texture_unit{
                texture floor_diffuse.PNG
            }
        }
    }
}

material alasAvion{
    technique{
        pass{
            ambient 0.5 0.5 0.5 
            diffuse 1.0 1.0 1.0
            texture_unit{
                texture terrain_detail.jpg
                color_op modulate
            }
            texture_unit{
                texture lightMap.jpg
                color_op modulate
            }
        }
    }
}
material example/smoke{
	technique{
		pass{
			lighting off
			scene_blend alpha_blend
			depth_write off
			diffuse vertexcolour
			texture_unit{
				texture smoke.png
				tex_address_mode clamp
			}
		}
	}
}